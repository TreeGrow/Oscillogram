<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.luntek.chiplink.test.oscillogramlib.TimingView
        android:id="@+id/timing"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.luntek.chiplink.test.oscillogramlib.CoordinatesView
        android:id="@+id/Coordinates"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="169.5dp"
        android:layout_marginEnd="63.5dp"
        app:layout_constraintBottom_toBottomOf="@+id/timing"
        app:layout_constraintEnd_toEndOf="@+id/timing"
        app:layout_constraintTop_toTopOf="@+id/timing" />

</androidx.constraintlayout.widget.ConstraintLayout>